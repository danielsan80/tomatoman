{% extends 'AcmeTomatomanBundle::layout.html.twig' %}
{% form_theme form 'AcmeTomatomanBundle:Form:form_div_layout.html.twig' %}

{% block content %}
    <h1>Project creation</h1>
    
    <form class="project_new" action="{{ path('admin_project_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">Create</button>
        </p>
    </form>
    
    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_project') }}">
                Back to the list
            </a>
        </li>
    </ul>
{% endblock %}
